<h2>Add new post</h2>
<%= form_with model: @post, url: user_posts_path , local: true do |f| %>

<div>
  <div>
    <%= f.label :title %> <br>
    <%= f.text_field :title %>
  </div>

  <div>
    <%= f.label :text %><br>
    <%= f.text_area :text %>
  </div>
  <%= f.submit %> <br>
</div>

<% end %>